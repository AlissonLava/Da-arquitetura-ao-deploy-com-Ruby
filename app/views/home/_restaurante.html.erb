<p>Seus produtos, <%= current_user.restaurante.name %></p>
<table>
            <thead>
                <tr>
                    <th>nome</th>
                    <th>valor</th>
                </th>
            </thead>

            <tbody>
                <% current_user.restaurante.products.each do |product| %> 
                    <tr>
                        <th>product.name</th>
                        <th>product.value</th>
                    </th>
                <%end%>
            </tbody>
        </table>